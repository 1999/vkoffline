{
	"name" : "__MSG_appName__ DEV",
	"version" : "5.0",
	"description" : "__MSG_appDescription__",
	"default_locale" : "ru",
	"icons" : {
		"128" : "pic/icon128.png",
		"48" : "pic/icon48.png",
		"16" : "pic/icon16.png"
	},
	"app": {
		"background": {
			"scripts": [
				"bower_components/lodash/dist/lodash.min.js",
				"bower_components/vow/vow.min.js",
				"bower_components/sklad/sklad.js",

				"js/app.js",
				"js/utils.js",

				"bower_components/cpa/google-analytics-bundle.js",
				"js/cpa.js",

				"js/soundManager.js",
				"js/storageManager.js",
				"js/accountsManager.js",
				"js/databaseManager.js",
				"js/settingsManager.js",
				"js/logManager.js",
				"js/cacheManager.js",
				"js/reqManager.js",
				"js/migrationManager.js",
				"js/backend.js"
			]
		}
	},

	"manifest_version" : 2,
	"offline_enabled" : true,
	"minimum_chrome_version" : "35",
	"permissions" : [
		"alarms",
		"unlimitedStorage",
		"storage",
		"notifications",
		"webview",
		"*://*.vk.com/*",
		"*://*.userapi.com/*",
		"*://*.vk.me/*"
	],
	"optional_permissions" : [
		"clipboardWrite"
	]
}
